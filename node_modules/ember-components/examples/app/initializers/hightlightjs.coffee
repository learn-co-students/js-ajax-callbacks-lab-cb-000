`import Em from 'ember'`

Highlightjs =
    name: 'hightlightjs'
    initialize: () ->
        Em.Route.reopen
            renderTemplate: ->
                @_super()
                Em.run.next @, ->
                    $('pre code').each((i, e) ->
                        hljs.highlightBlock(e)
                    )

`export default Highlightjs`
