`import Ember from "ember"`

OpenIfCloseIf = Ember.Controller.extend
    openModal: false
    closeModal: false
    message: undefined
    actions:
        raise: ->
            @set 'openModal', true
        save: ->
            if @get('checkMe') then @set('closeModal', true) else @set('message', 'Check the checkbox first!')
`export default OpenIfCloseIf`