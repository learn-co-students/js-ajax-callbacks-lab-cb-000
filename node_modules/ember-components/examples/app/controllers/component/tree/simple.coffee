`import Ember from "ember"`

Simple = Ember.ObjectController.extend
    init: ->
        #Root
        #TODO: Import EC instead
        family = Em.EC.TreeNode.create({title: 'Family'})
        #1st level
        suz = family.createChild {title: 'Susan'}
        lud = family.createChild {title: 'Luda'}

        #2nd level
        josh = suz.createChild({title: 'Josh'})
        moses = suz.createChild({title: 'Moses'})
        verdi = lud.createChild({title: 'Verdi'})
        gaya = lud.createChild({title: 'Gaya'})

        @set 'model', family


    actions: 
        expand: ->
            @get('selected').toggleProperty 'expanded'
            return null
`export default Simple`